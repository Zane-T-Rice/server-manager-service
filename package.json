{
  "name": "server-manager-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node build/src/app.js",
    "start:dev": "npx nodemon -x 'ts-node src/app.ts | pino-pretty' -w './src' -e 'ts'",
    "lint": "npx eslint src; npx eslint test;",
    "test": "rm -rf build && npm run build && jest --coverage --verbose --passWithNoTests --rootDir build",
    "migrate": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev --name init"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@types/pino-http": "^5.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "http-errors": "~1.6.3",
    "pino-http": "^10.3.0",
    "shell-escape": "^0.2.0",
    "swagger-ui-dist": "^5.17.14",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/eslintrc": "^3.1.0",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^9.6.1",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.1",
    "@types/shell-escape": "^0.2.3",
    "@types/swagger-ui-dist": "^3.30.5",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.14.0",
    "git-hooks": "^1.1.10",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "jiti": "^2.3.3",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
